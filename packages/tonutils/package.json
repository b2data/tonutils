{
  "name": "tonutils",
  "version": "1.0.0",
  "description": "A comprehensive toolkit for working with the TON (The Open Network) protocols.",
  "keywords": [
    "ton",
    "blockchain",
    "networking",
    "protocol",
    "typescript",
    "toolkit"
  ],
  "author": "thekiba <hr@thekiba.io>, goreactdev <goreactdev12@gmail.com>",
  "homepage": "https://github.com/thekiba/tonutils#readme",
  "license": "MIT",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    "./adnl": {
      "types": "./adnl.d.ts",
      "import": "./adnl.js",
      "require": "./adnl.js"
    },
    "./config": {
      "types": "./config.d.ts",
      "import": "./config.js",
      "require": "./config.js"
    },
    "./dht": {
      "types": "./dht.d.ts",
      "import": "./dht.js",
      "require": "./dht.js"
    },
    "./fec": {
      "types": "./fec.d.ts",
      "import": "./fec.js",
      "require": "./fec.js"
    },
    "./http": {
      "types": "./http.d.ts",
      "import": "./http.js",
      "require": "./http.js"
    },
    "./keyring": {
      "types": "./keyring.d.ts",
      "import": "./keyring.js",
      "require": "./keyring.js"
    },
    "./keys": {
      "types": "./keys.d.ts",
      "import": "./keys.js",
      "require": "./keys.js"
    },
    "./overlay": {
      "types": "./overlay.d.ts",
      "import": "./overlay.js",
      "require": "./overlay.js"
    },
    "./raptorq": {
      "types": "./raptorq.d.ts",
      "import": "./raptorq.js",
      "require": "./raptorq.js"
    },
    "./rldp": {
      "types": "./rldp.d.ts",
      "import": "./rldp.js",
      "require": "./rldp.js"
    },
    "./rldp2": {
      "types": "./rldp2.d.ts",
      "import": "./rldp2.js",
      "require": "./rldp2.js"
    },
    "./storage": {
      "types": "./storage.d.ts",
      "import": "./storage.js",
      "require": "./storage.js"
    },
    "./tl": {
      "types": "./tl.d.ts",
      "import": "./tl.js",
      "require": "./tl.js"
    },
    "./tlb": {
      "types": "./tlb.d.ts",
      "import": "./tlb.js",
      "require": "./tlb.js"
    },
    "./package.json": "./package.json"
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thekiba/tonutils.git"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "jest"
  },
  "bugs": {
    "url": "https://github.com/thekiba/tonutils/issues"
  },
  "dependencies": {
    "@tonutils/adnl": "^0.0.0",
    "@tonutils/config": "^0.0.0",
    "@tonutils/dht": "^0.0.0",
    "@tonutils/fec": "^0.0.0",
    "@tonutils/http": "^0.0.0",
    "@tonutils/keyring": "^0.0.0",
    "@tonutils/keys": "^0.0.0",
    "@tonutils/overlay": "^0.0.0",
    "@tonutils/raptorq": "^0.0.0",
    "@tonutils/rldp": "^0.0.0",
    "@tonutils/rldp2": "^0.0.0",
    "@tonutils/storage": "^0.0.0",
    "@tonutils/tl": "^0.0.0",
    "@tonutils/tlb": "^0.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.13",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0"
  }
}
